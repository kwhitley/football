{
  "name": "dropbox-gallery",
  "version": "1.0.0",
  "description": "for painless photo gallery creation",
  "title": "Dropbox Gallery",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/server.js",
  "target": "esnext",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 --inspect dist/server.js",
    "test": "mocha -r esm -r ts-node/register src/**/*.spec.*",
    "predev": "rm -rf .fusebox cache",
    "dev": "node fuse",
    "build": "node fuse build",
    "lint": "eslint src --ext .js,.jsx",
    "setup": "node ./.dbscripts/init_db.js && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.2.3",
    "app-root-path": "^2.1.0",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "dropbox": "^4.0.15",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "globby": "^9.0.0",
    "isomorphic-fetch": "^2.2.1",
    "manage-users": "^1.2.1",
    "mongodb": "^3.1.12",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.21.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "esm": "^3.1.1",
    "fuse-box": "^3.6.0",
    "fuse-test-runner": "^1.0.16",
    "history": "^4.7.2",
    "less": "^3.9.0",
    "mobx": "^5.8.0",
    "mobx-preact": "^3.0.0",
    "mobx-react": "^5.4.3",
    "mobx-react-router": "^4.0.5",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "prop-types": "^15.6.2",
    "react": ">=16.7.0",
    "react-autosize-textarea": "^6.0.0",
    "react-dom": ">=16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-markings": "^1.3.0",
    "react-router-dom": "^4.3.1",
    "typescript": "^3.2.4",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.9"
  }
