{
  "name": "dropbox-gallery",
  "version": "1.0.0",
  "description": "for painless photo gallery creation",
  "title": "Dropbox Gallery",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/server.js",
  "target": "esnext",
  "scripts": {
    "start": "node --optimize_for_size --max_old_space_size=460 --inspect dist/server.js",
    "test": "mocha -r esm -r ts-node/register src/**/*.spec.*",
    "predev": "rm -rf .fusebox cache",
    "dev": "node fuse",
    "build": "node fuse build",
    "lint": "eslint src --ext .js,.jsx",
    "setup": "node ./.dbscripts/init_db.js && npm run test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apicache": "^1.2.3",
    "app-root-path": "^2.1.0",
    "async": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "dropbox": "^4.0.15",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "globby": "^9.0.0",
    "heroku-ssl-redirect": "^0.0.4",
    "isomorphic-fetch": "^2.2.1",
    "javascript-blowfish": "^1.0.4",
    "mongodb": "^3.1.13",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.21.3",
    "supergeneric": "^1.1.0",
    "xs-blowfish": "^2.0.0"
  },
  "devDependencies": {
    "@reach/router": "^1.2.1",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "esm": "^3.2.1",
    "fuse-box": "^3.7.0",
    "fuse-test-runner": "^1.0.16",
    "history": "^4.7.2",
    "less": "^3.9.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "preact": "^8.4.2",
    "preact-compat": "^3.18.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.1",
    "react-autosize-textarea": "^6.0.0",
    "react-cache": "^2.0.0-alpha.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-markings": "^1.3.0",
    "typescript": "^3.3.1",
    "uglify-es": "^3.3.9",
